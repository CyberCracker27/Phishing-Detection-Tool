{
    "manifest_version": 3,
    "name": "Phishing Detector",
    "version": "2.0",
    "permissions": ["scripting", "activeTab", "storage"],
    "host_permissions": ["https://mail.google.com/*"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://mail.google.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "128": "icon128.png"
    }
  }
  